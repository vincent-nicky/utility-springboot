<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsj.dao.GoodsDao">
    <select id="findAll" resultType="com.wsj.entity.Goods">
        select id, title, price, stock, saleNum,
        createTime, categoryName, brandName, spec specStr
        from goods
    </select>
</mapper>